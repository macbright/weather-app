{
  "parserOptions": { "ecmaVersion": 6 },
  "extends": "airbnb-base",
    rules: {
    'no-shadow': 0,
    'no-param-reassign': 0,
		  "comma-dangle": ["error", "never"],
    'eol-last': 0,
    "indent": [2, "tab"],
    'no-tabs': 0,
    'skipBlankLines': true, 
		'allowShortCircuit': true,
    'allowTernary': true
  }
}